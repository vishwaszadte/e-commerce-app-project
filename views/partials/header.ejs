<div>
    <% if (user && user.userType === 2) {%>
        <div class="header">
            <h3><%= user.username %></h3>
            <img class="user-profile-pic" src=" <%= user.profile_pic %> " alt="">
            <button><a href="/logout">Logout</a></button>
        </div>
    <% } else {%>
        <a href="/auth">login</a>
    <% }%>
    
</div>